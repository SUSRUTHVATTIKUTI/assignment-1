#<stdio.h>
void printorder(int A[]; int N){
int i,j,k;
for(i=0;i<n;i++){
if(A[i]==(-1)){
printf("%d",i);
k=i;
}
}
for(j=1;j<n;j++){
for(i=0;i<n;i++){
if(A[i]==k){
printf("%d",i);
k=i;
}
}
}
}
int main(){
int N;
printf("enter the number of people in the line :");
snanf("%d",&N);
